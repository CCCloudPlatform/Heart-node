{
  "name": "node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "------------------------BUILDS------------------------": "",
    "build": "npm run build:sdk && npm run build:main && npm run build:test",
    "build:main": "rimraf dist && tsc",
    "build:sdk": "rimraf src/api/functional && nestia sdk",
    "build:test": "rimraf bin && tsc -p test/tsconfig.json",
    "build:swagger": "nestia swagger && ts-node src/executable/indexing_swagger_files.ts",
    "dev": "npm run build:test -- --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "------------------------DEPLOYS------------------------": "",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:local": "cross-env NODE_ENV=local node dist/main",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "-------------------------TEST-------------------------": "",
    "test": "cross-env NODE_ENV=test node bin/test",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "-------------------------ECT-------------------------": "",
    "prepare": "ts-patch install && typia patch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "package:api": "npm run build:api && cd packages/api && npm publish"
  },
  "dependencies": {
    "@nestia/core": "^3.18.0",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.18.0",
    "@nestia/sdk": "^3.18.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-zipkin": "^1.28.0",
    "@opentelemetry/instrumentation": "^0.55.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "axios": "^1.7.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "tstl": "^3.0.0",
    "typia": "^6.11.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/axios": "^0.14.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^6.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-transform-paths": "^3.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
